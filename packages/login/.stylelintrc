{
  "processors": ["stylelint-processor-styled-components"],
  "extends": [
    "stylelint-config-recommended",
    "stylelint-config-styled-components"
  ],
  "rules": {
    "color-no-hex": true,
    "color-named": "never",
    "no-descending-specificity": null,
    "property-no-vendor-prefix": null,
    "opencrvs/no-font-styles": true
  },
  "plugins": [
    "./no-font-styles.js"
  ]
}
